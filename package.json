{
  "name": "tomhendra-portfolio",
  "version": "0.0.0",
  "private": true,
  "description": "A portfolio website for Tom Hendra, web developer",
  "author": "Tom Hendra <hola@tomhendra.dev>",
  "keywords": [
    "tom hendra"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomhendra/portfolio"
  },
  "engines": {
    "node": "10.x"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "start": "lerna run empty-cache && lerna run start --parallel watch",
    "build": "lerna run build --parallel",
    "deploy:graphql": "lerna run deploy:graphql",
    "deploy:sanity": "lerna run deploy:sanity",
    "export:sanity": "cd backend && yarn export",
    "format": "lerna run format",
    "lint": "lerna run lint",
    "test": "echo \"Error: no test specified, write some tests!\" && exit 1",
    "empty-cache": "cd frontend && yarn empty-cache",
    "clean": "lerna clean",
    "update": "lernaupdate",
    "init": "cd backend && sanity init && lerna run deploy:graphql",
    "bootstrap": "lerna bootstrap --use-workspaces",
    "postinstall": "lerna bootstrap --use-workspaces"
  },
  "devDependencies": {
    "lerna": "^3.17.0",
    "lerna-update-wizard": "0.16.0"
  },
  "dependencies": {}
}

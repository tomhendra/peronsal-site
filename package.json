{
  "name": "tomhendra.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier \"**/*.+(js|jsx|json|yml|yaml|ts|tsx|md|mdx|gql|graphql)\" . --write",
    "validate": "npm-run-all --parallel lint format build",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "nuke": "rm -rf .next && rm -rf node_modules && rm pnpm-lock.yaml"
  },
  "dependencies": {
    "next": "13.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^1.13.2",
    "tailwindcss": "3.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "18.16.3",
    "@types/react": "18.2.5",
    "@types/react-dom": "18.2.3",
    "@types/testing-library__jest-dom": "^5.14.6",
    "autoprefixer": "10.4.14",
    "eslint": "8.39.0",
    "eslint-config-next": "13.4.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "8.4.23",
    "prettier": "^2.8.8",
    "typescript": "5.0.4"
  }
}

{
  "private": true,
  "name": "studio",
  "description": "Sanity studio CMS for Tom Hendra portfolio website.",
  "license": "MIT",
  "author": "Tom Hendra <hola@tomhendra.dev>",
  "repository": "https://github.com/tomhendra/portfolio",
  "version": "2.0.0",
  "scripts": {
    "start": "sanity start",
    "build": "sanity build",
    "test": "sanity check",
    "dataset:export-prod": "rimraf export && sanity dataset export production export/production.tar.gz",
    "dataset:import-prod": "sanity dataset import export/production.tar.gz development --replace",
    "deploy:graphql-dev": "sanity graphql deploy --dataset development --generation gen2 --playground",
    "deploy:graphql-prod": "sanity graphql deploy --dataset production --generation gen2 --no-playground",
    "deploy:graphql": "yarn deploy:graphql-prod && yarn deploy:graphql-dev",
    "deploy:sanity": "sanity deploy",
    "init": "sanity init && yarn deploy:graphql"
  },
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@emotion/core": "10.0.35",
    "@sanity/base": "^1.150.5",
    "@sanity/cli": "^1.150.1",
    "@sanity/code-input": "^1.150.1",
    "@sanity/components": "^1.150.5",
    "@sanity/core": "^1.150.3",
    "@sanity/dashboard": "^1.150.4",
    "@sanity/default-layout": "^1.150.5",
    "@sanity/default-login": "^1.150.1",
    "@sanity/desk-tool": "^1.150.5",
    "date-fns": "^2.16.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sanity-plugin-google-analytics": "^0.2.6"
  },
  "keywords": [
    "tom hendra"
  ],
  "main": "index.js"
}

    "@sanity/default-login": "^0.146.0",
    "@sanity/desk-tool": "^0.146.3",
    "date-fns": "^2.8.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.0"

{
  "private": true,
  "name": "frontend",
  "description": "Gatsby frontend for Tom Hendra portfolio website.",
  "license": "MIT",
  "author": "Tom Hendra <hola@tomhendra.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomhendra/portfolio"
  },
  "version": "0.0.2",
  "scripts": {
    "develop": "yarn empty-cache && GATSBY_GRAPHQL_IDE=playground gatsby develop -H $HOSTNAME --port 3000 --open 'Google Chrome'",
    "start": "run-p start:**",
    "start:app": "yarn develop",
    "build": "run-p build:**",
    "build:app": "gatsby build",
    "format": "prettier --write \"**/*.js\" \"!.cache/**\" \"!node_modules/**\" \"!public/**\"",
    "lint": "eslint . --fix",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "empty-cache": "rm -rf .cache",
    "clean": "gatsby clean",
    "serve": "gatsby serve",
    "info": "gatsby info --clipboard"
  },
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@emotion/core": "10.0.21",
    "@sanity/block-content-to-react": "^2.0.7",
    "date-fns": "^2.4.1",
    "dotenv": "^8.1.0",
    "emotion-theming": "^10.0.19",
    "formik": "^1.5.8",
    "framer-motion": "^1.6.12",
    "gatsby": "^2.15.35",
    "gatsby-image": "^2.2.26",
    "gatsby-plugin-emotion": "^4.1.10",
    "gatsby-plugin-manifest": "^2.2.21",
    "gatsby-plugin-offline": "^3.0.14",
    "gatsby-plugin-react-helmet": "^3.1.11",
    "gatsby-plugin-sharp": "^2.2.29",
    "gatsby-source-filesystem": "^2.1.31",
    "gatsby-source-sanity": "^5.0.3",
    "gatsby-transformer-sharp": "^2.2.21",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "typeface-lato": "0.0.75",
    "typeface-noto-sans": "0.0.72",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "keywords": [
    "tom hendra"
  ],
  "engines": {
    "node": "10.x"
  }
}

{
  "name": "tomhendra-portfolio-frontend",
  "version": "0.0.2",
  "private": false,
  "description": "A portfolio website for Tom Hendra, web developer",
  "author": "Tom Hendra <hola@tomhendra.dev>",
  "keywords": [
    "tom hendra"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomhendra/portfolio"
  },
  "engines": {
    "node": "10.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src /**/*.js": [
      "eslint",
      "pretty-quick â€”-staged",
      "git add"
    ]
  },
  "scripts": {
    "develop": "yarn empty-cache && gatsby develop -H $HOSTNAME --port 3000 --open 'Google Chrome'",
    "start": "run-p start:**",
    "start:app": "yarn develop",
    "build": "run-p build:**",
    "build:app": "gatsby build",
    "serve": "gatsby serve",
    "info": "gatsby info --clipboard",
    "format": "prettier --write \"**/*.js\" \"!.cache/**\" \"!node_modules/**\" \"!public/**\"",
    "lint": "eslint .",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "empty-cache": "rimraf .cache",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@emotion/core": "^10.0.21",
    "@sanity/block-content-to-react": "^2.0.7",
    "date-fns": "^2.4.1",
    "dotenv": "^8.1.0",
    "formik": "^1.5.8",
    "framer-motion": "^1.6.8",
    "gatsby": "^2.15.29",
    "gatsby-image": "^2.2.24",
    "gatsby-plugin-emotion": "^4.1.9",
    "gatsby-plugin-manifest": "^2.2.20",
    "gatsby-plugin-offline": "^3.0.11",
    "gatsby-plugin-react-helmet": "^3.1.10",
    "gatsby-plugin-sharp": "^2.2.28",
    "gatsby-source-filesystem": "^2.1.29",
    "gatsby-source-sanity": "^5.0.3",
    "gatsby-transformer-sharp": "^2.2.20",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "typeface-lato": "0.0.75",
    "typeface-noto-sans": "0.0.72",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-import": "2.18.2",
    "husky": "3.0.8",
    "lint-staged": "9.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "rimraf": "^3.0.0"
  }
}

    "polished": "^3.4.2",
    "prism-react-renderer": "^1.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-feather": "^2.0.3",
    "react-helmet": "^5.2.1",
    "typeface-lato": "0.0.75",
    "typeface-noto-sans": "0.0.72",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"

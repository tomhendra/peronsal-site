{
  "name": "tomhendra-portfolio-sanity-studio",
  "version": "0.0.1",
  "private": false,
  "description": "A portfolio website for Tom Hendra, web developer",
  "author": "Tom Hendra <hola@tomhendra.dev>",
  "keywords": [
    "tom hendra"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomhendra/portfolio"
  },
  "engines": {
    "node": "10.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src /**/*.js": [
      "eslint",
      "pretty-quick â€”-staged",
      "git add"
    ]
  },
  "scripts": {
    "start": "sanity start",
    "build": "sanity build",
    "deploy:graphql": "sanity graphql deploy --playground --open 'Google Chrome",
    "deploy:sanity": "sanity deploy",
    "format": "prettier --write \"**/*.js\" \"!node_modules/**\"",
    "lint": "eslint .",
    "test": "sanity check",
    "export": "rimraf export && sanity dataset export production export/production.tar.gz"
  },
  "dependencies": {
    "@sanity/base": "^0.142.6",
    "@sanity/cli": "0.142.7",
    "@sanity/components": "^0.142.6",
    "@sanity/core": "^0.142.6",
    "@sanity/default-layout": "^0.142.7",
    "@sanity/default-login": "^0.142.6",
    "@sanity/desk-tool": "^0.142.7",
    "date-fns": "^2.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-import": "2.18.2",
    "husky": "3.0.8",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "rimraf": "^3.0.0"
  }
}
